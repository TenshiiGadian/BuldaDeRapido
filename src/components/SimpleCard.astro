---
interface Props {
  title?: string;
  description?: string;
}
const { title = "Default Title", description = "Desc default" } = Astro.props;
---

<div class="bg-[#FFA851] rounded px-12 py-5 shadow-2xl">
  <h3 class="text-xl font-bold">{title}</h3>
  <p class="text-lg">
    {description}
  </p>
</div>
